    "grommet": "^1.11.0",
    "immutable": "^3.8.2",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
