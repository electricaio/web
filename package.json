{
  "name": "electrica-web",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "watch": "webpack-dev-server --open --config webpack.dev.js",
    "test": "jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test:watch": "NODE_ENV=test jest --watchAll",
    "eslint": "eslint -c ./.eslintrc.js \"./*.js\"",
    "tslint": "tslint -c ./tslint.json \"src/**/*.tsx\" \"src/**/*.ts\" --project ./tsconfig.json",
    "lint": "npm run eslint && npm run tslint",
    "format": "npm run format:js && npm run format:ts",
    "format:ts": "prettier --write \"src/**/*.tsx\" \"src/**/*.ts\" && npm run tslint",
    "format:js": "prettier --write \"src/**/*.js\" \"/*.js\" \"webpack*.js\" && npm run eslint",
    "reinstall": "rm -rf node_modules && npm i",
    "precommit": "npm run format && npm run lint && npm run test:coverage"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/jest": "^23.3.5",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.9",
    "@types/react-redux": "^6.0.9",
    "@types/react-router-dom": "^4.3.1",
    "@types/redux-promise": "^0.5.28",
    "@types/styled-components": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.6.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-app": "^3.0.3",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.0",
    "prettier": "1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "react-svg-loader": "^2.1.0",
    "redux-devtools": "^3.4.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "tslint-config-airbnb-base": "^0.2.0",
    "typescript": "^3.1.2",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "antd": "^3.10.0",
    "date-fns": "^1.29.0",
    "enzyme": "^3.7.0",
    "fp-ts": "^1.9.0",
    "immutable": "^3.8.2",
    "jest-svg-transformer": "^1.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-promise-middleware": "^5.1.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^3.4.9",
    "tslint-config-prettier": "^1.15.0",
    "typelevel-ts": "^0.3.2"
  },
  "jest": {
    "transform": {
      ".*.tsx?$": "ts-jest",
      "^.+\\.svg$": "jest-svg-transformer"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts?(x)"
    ],
    "setupTestFrameworkScriptFile": "./config/jestSetup.js"
  }
}
