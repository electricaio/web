{
  "presets": ["es2015", "babel-preset-env", "react"],
  "plugins": ["transform-class-properties"],
  "env": {
    "plugins": [
      [
        "import",
        { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }
      ]
    ],
    "production": {
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "wrap"
          }
        ]
      ]
    }
  }
}
