{
    "env": {
      "plugins": [
        [
          "import",
          { "libraryName": "antd", "libraryDirectory": "es", "style": "css" }
        ]
      ],
      "test": {
        "presets": ["babel-preset-env", "react"]
      },
      "development": {
        "presets": ["babel-preset-env", "react"]
      },
      "production": {
        "presets": ["babel-preset-env", "react"],
        "plugins": [
          [
            "transform-react-remove-prop-types",
            {
              "mode": "wrap"
            }
          ]
        ]
      }
    }
  }
  